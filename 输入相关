//数组
int mk[100];
int i=0;
while(cin >> mk[i])
	{
			i++;
			if (cin.get() == '\n')
				break;
     }

//vector
vector<int> v;
	int a;
	while(cin >> a)
	{
		v.push_back(a);
		char c = cin.get();
		if(c == '\n')
			break;
		
	}
	cout<<v.size()<<endl;
	int i = 0;
	while(v.size() > i)
	{
		cout<<v[i++]<<" ";
	}
--------------------- 
